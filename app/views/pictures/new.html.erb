<h1>Upload</h1>

<form action="<%= pictures_url %>"
  method="POST"
  enctype="multipart/form-data">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <label>Title<input type="text" name="picture[title]"></label>
  <label>Description<textarea name="picture[description]"></textarea>
  <label>File<input type="file" name="picture[img_url]"></label>

  <button>Upload</button>
</form>